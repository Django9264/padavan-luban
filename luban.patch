diff --git a/trunk/linux-4.4.x/arch/mips/boot/dts/ralink/luban.dts b/trunk/linux-4.4.x/arch/mips/boot/dts/ralink/luban.dts
new file mode 100644
index 0000000..d16b135
--- /dev/null
+++ b/trunk/linux-4.4.x/arch/mips/boot/dts/ralink/luban.dts
@@ -0,0 +1,123 @@
+/dts-v1/;
+
+#include "mt7621.dtsi"
+#include <dt-bindings/input/input.h>
+#include "mt7621-raeth-gmac2.dtsi"
+#include "mt7621-pinctrl-default-gpio.dtsi"
+
+/ {
+	compatible = "mediatek,mt7621-soc";
+	model = "JDCloud LuBan";
+
+	chosen {
+		bootargs = "console=ttyS0,115200";
+	};
+
+	leds {
+		compatible = "gpio-leds";
+	
+		stat_r {
+			label = "red";
+			gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
+		};
+
+		stat_g {
+			label = "wan";
+			gpios = <&gpio0 8 GPIO_ACTIVE_LOW>;
+		};
+
+		stat_b {
+			label = "power";
+			gpios = <&gpio0 7 GPIO_ACTIVE_LOW>;
+		};
+
+	};
+	
+	gpio-keys-polled {
+		compatible = "gpio-keys-polled";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		poll-interval = <20>;
+
+		reset {
+			label = "reset";
+			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
+			linux,code = <KEY_RESTART>;
+		};
+
+		wps {
+			label = "wps";
+			gpios = <&gpio0 15 GPIO_ACTIVE_LOW>;
+			linux,code = <KEY_WPS_BUTTON>;
+		};
+	};
+};
+
+&spi0 {
+	status = "okay";
+
+	m25p80@0 {
+		#address-cells = <1>;
+		#size-cells = <1>;
+		compatible = "jedec,spi-nor";
+		reg = <0x00>;
+		spi-max-frequency = <10000000>;
+
+		partition@0 {
+			label = "Bootloader";
+			reg = <0x00 0x40000>;
+		};
+
+		partition@40000 {
+			label = "Config";
+			reg = <0x40000 0x10000>;
+		};
+
+		partition@50000 {
+			label = "Factory";
+			reg = <0x50000 0x40000>;
+		};
+
+		partition@90000 {
+			label = "firmware";
+			reg = <0x90000 0xef0000>;
+		};
+
+		partition@f80000 {
+			label = "Storage";
+			reg = <0xf80000 0x80000>;
+		};
+
+		partition@all {
+			label = "ALL";
+			reg = <0x00 0x1000000>;
+		};
+	};
+};
+
+&state_default {
+	gpio {
+		mtk,group = "i2c","uart2","uart3","jtag","wdt";
+		mtk,function = "gpio";
+	};
+};
+
+&raeth {
+	status = "okay";
+};
+
+&hnat {
+	status = "okay";
+	mtketh-wan = "eth1";
+	mtketh-ppd = "eth0";
+	ext-devices = "rax0","ra0","rax1","ra1","rax2","ra2","rax3","ra3","apclix0","apcli0";
+	mtketh-max-gmac = <2>;
+};
+
+&sdhci {
+	status = "okay";
+};
+
+&pcie {
+	status = "okay";
+};
